class Solution {
    public int[] productExceptSelf(int[] nums) {
        int[] res=new int[nums.length];
        res[0]=1;
        int n=nums.length;
        for(int i=0;i<nums.length-1;i++){
            res[i+1]=nums[i]*res[i];
        }
        // [1,1,2,6]

        int rightProduct = 1;
        for (int i = n - 1; i >= 0; i--) {
            res[i] *= rightProduct;
            rightProduct *= nums[i];
        }
        // [24,12,8,6]

        return res;
    }
}
